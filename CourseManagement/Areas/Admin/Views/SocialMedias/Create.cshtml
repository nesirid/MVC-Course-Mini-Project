@model CourseManagement.ViewModels.SocialMediaVM

@{
    ViewData["Title"] = "Create Social Media";
}
<section>

    <div class="mt-5">
        <h1>Create Social Media</h1>
        <div>
            <a class="btn btn-primary" asp-action="Index">Back to List</a>
        </div>
    </div>
    <div class="col">
        <div>
            <form asp-action="Create" enctype="multipart/form-data">
                <div class="form-group">
                    <label asp-for="Name" class="control-label"></label>
                    <input asp-for="Name" class="form-control" />
                    <span asp-validation-for="Name" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="IconFile" class="control-label"></label>
                    <input asp-for="IconFile" type="file" class="form-control" />
                    <span asp-validation-for="IconFile" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <input type="submit" value="Create" class="btn btn-primary" />
                </div>
            </form>
        </div>
    </div>
</div>


</section>


